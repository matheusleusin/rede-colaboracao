@CONFERENCE{Kalnikaite201111,
author={Kalnikaite, V. and Rogers, Y. and Bird, J. and Villar, N. and Bachour, K. and Payne, S. and Todd, P.M. and Schöning, J. and Krüger, A. and Kreitmayer, S.},
title={How to nudge in situ: Designing lambent devices to deliver salient information in supermarkets},
