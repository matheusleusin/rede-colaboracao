@CONFERENCE{Krüger200911,
author={Krüger, A. and Schöning, J. and Löchtefeld, M. and Steinicke, F. and Rohs, M.},
title={About two physical interaction metaphors: Narrowing the gap between the virtual and the real world},
journal={Proceedings - 2009 International Symposium on Ubiquitous Virtual Reality, ISUVR 2009},
year={2009},
pages={11-14},
doi={10.1109/ISUVR.2009.25},
art_number={5232245},
note={cited By 0; Conference of 2009 International Symposium on Ubiquitous Virtual Reality, ISUVR 2009 ; Conference Date: 8 July 2009 Through 11 July 2009;  Conference Code:78042},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-70350548366&doi=10.1109%2fISUVR.2009.25&partnerID=40&md5=29332b35fd61b55e9fc9f7a6d830a85f},
affiliation={German Research Center for Artificial Intelligence (DFKI), Germany; University of Münster, Germany; Deutsche Telekom Laboratories, Berlin, Germany},
abstract={In this paper we present ideas that can help to close the gap between virtual reality and embedded interaction, which are usually assumed to be the two extremes of a dimension that describes mixed reality interaction. We present our preliminary ideas on surface interaction with stereoscopic data as well as our work on mobile camera-projector units. While the first line of research tries to increase the degree of the interaction experience in virtual worlds, the second uses the physical properties of the real world to enhance the augmented reality experience. The first idea uses the physical world to constraint the interaction in the virtual world. The second idea embeds virtual information into the physical world by respecting its physical properties. © 2009 IEEE.},
