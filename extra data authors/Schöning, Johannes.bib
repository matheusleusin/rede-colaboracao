@ARTICLE{Savino2020,
author={Savino, G.-L. and Sturdee, M. and Rundé, S. and Lohmeier, C. and Hecht, B. and Prandi, C. and Nunes, N.J. and Schöning, J.},
title={MapRecorder: analysing real-world usage of mobile map applications},
journal={Behaviour and Information Technology},
year={2020},
doi={10.1080/0144929X.2020.1714733},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85079382826&doi=10.1080%2f0144929X.2020.1714733&partnerID=40&md5=719c02ba95b66b3d8ba28f84b52e18aa},
affiliation={Human-Computer Interaction, University of Bremen, Bremen, Germany; Computing and Communications, Lancaster University, Lancaster, United Kingdom; Communication, University of Salzburg, Salzburg, Austria; People, Space and Algorithms, Northwestern University, Evanston, IL, United States; ITI/LARSyS, Madeira, Portugal},
abstract={Millions of people use mobile map applications like Google Maps on a regular basis. However, despite these applications' ubiquity, the literature contains very little information about how these applications are used in the real world. As such, many researchers and practitioners seeking to improve mobile map applications may not be able to identify important challenges and may miss major opportunities for innovation. To address this paucity of usage information, we collected and analysed data during unsupervised usage of Google Maps by replacing the standard application with a wrapped version called MapRecorder. In two studies we recorded data from locals and tourists using our application and collected over 580 minutes of actual application usage from 34 users, spanning 555 unique sessions. We identify typical usage scenarios, observe a large amount of map exploration and elucidate generalisable interaction patterns. © 2020, © 2020 Informa UK Limited, trading as Taylor & Francis Group.},
author_keywords={actual use;  human-computer interaction;  Mobile map usage;  navigation;  user need analysis},
