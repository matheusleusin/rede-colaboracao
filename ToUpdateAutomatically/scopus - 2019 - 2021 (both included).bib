@ARTICLE{Goli2020492,
author={Goli, M. and Stoppe, J. and Drechsler, R.},
title={Automated Nonintrusive Analysis of Electronic System Level Designs},
journal={IEEE Transactions on Computer-Aided Design of Integrated Circuits and Systems},
year={2020},
