@CONFERENCE{Zulehner20191,
author={Zulehner, A. and Niemann, P. and Drechsler, R. and Wille, R.},
title={One Additional Qubit is Enough: Encoded Embeddings for Boolean Components in Quantum Circuits},
journal={Proceedings of The International Symposium on Multiple-Valued Logic},
