@ARTICLE{Stiefler202060,
author={Stiefler, S. and Seibert, K. and Domhoff, D. and Görres, S. and Wolf-Ostermann, K. and Peschke, D.},
title={Influencing factors for nursing home admission in case of pre-existing need of care – a systematic review [Einflussfaktoren für einen Heimeintritt bei bestehender Pflegebedürftigkeit – eine systematische Übersichtsarbeit]},
journal={Zeitschrift fur Evidenz, Fortbildung und Qualitat im Gesundheitswesen},
year={2020},
volume={153-154},
