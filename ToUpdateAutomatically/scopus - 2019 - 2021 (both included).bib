@ARTICLE{Torres2019,
author={Torres, F.S. and Niemann, P. and Wille, R. and Drechsler, R.},
title={Near zero-energy computation using quantum-dot cellular automata},
journal={ACM Journal on Emerging Technologies in Computing Systems},
year={2019},
