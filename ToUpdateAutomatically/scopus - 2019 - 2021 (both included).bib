@CONFERENCE{Hubner2019,
author={Hubner, M. and Schafer, H. and Koschek, K. and Lang, W.},
title={Online monitoring of shape memory polymers with a material integrated flexible interdigital sensor},
journal={Proceedings of IEEE Sensors},
year={2019},
volume={2019-October},
doi={10.1109/SENSORS43011.2019.8956514},
art_number={8956514},
note={cited By 0; Conference of 18th IEEE Sensors, SENSORS 2019 ; Conference Date: 27 October 2019 Through 30 October 2019;  Conference Code:156870},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85078703705&doi=10.1109%2fSENSORS43011.2019.8956514&partnerID=40&md5=5016d08a0d3e58dfe33dd88589a00820},
affiliation={University Bremen, Institute for Microsensors Actuators, and Systems, Bremen, Germany; Fraunhofer Institute for Manufacturing Technology and Advanced Materials IFAM, Bremen, Germany},
abstract={Thermoset thermoplastic mixtures can be used to fabricate shape memory polymer actuators. For most applications, the current state of an actuator during operation must be known e.g. for movement control. In this work, we present a miniaturized dielectric sensor, which enables online monitoring of the different states of a thermoset thermoplastic actuator. The impedance of the sensor strongly correlates with the internal structure of the shape memory polymer and, thus, can be used for the determination of the current switching state of the material. Additionally, impedance measurements give insights on the internal reorientation mechanisms and processes of the polymer. The dielectric sensor has great potential to be used for continuous monitoring of shape memory polymers during actuation and, therefore, could largely promote industrial applications. © 2019 IEEE.},
