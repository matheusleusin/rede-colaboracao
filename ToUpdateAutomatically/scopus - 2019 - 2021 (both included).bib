@ARTICLE{Toenjes2019,
author={Toenjes, A. and Sonnenberg, H. and Plump, C. and Drechsler, R. and von Hehl, A.},
title={Measurement and evaluation of calorimetric descriptors for the suitability for evolutionary high-throughput material development},
journal={Metals},
year={2019},
volume={9},
