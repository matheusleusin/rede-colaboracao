@ARTICLE{Soliman201932,
author={Soliman, M. and Siemons, J. and Kochems, J. and Alshrafi, W. and Shamshoom, J. and Heberling, D. and Dekorsy, A.},
title={Automatic Train Coupling: Challenges and Key Enablers},
