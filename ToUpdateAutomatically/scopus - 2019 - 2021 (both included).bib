@ARTICLE{Boskamp20201301,
author={Boskamp, T. and Lachmund, D. and Casadonte, R. and Hauberg-Lotte, L. and Kobarg, J.H. and Kriegsmann, J. and Maass, P.},
title={Using the chemical noise background in maldi mass spectrometry imaging for mass alignment and calibration},
