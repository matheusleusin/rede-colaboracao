@ARTICLE{Schulz2020,
author={Schulz, M. and Czwikla, J. and Tsiasioti, C. and Schwinger, A. and Gand, D. and Schmiemann, G. and Schmidt, A. and Wolf-Ostermann, K. and Kloep, S. and Heinze, F. and Rothgang, H.},
title={Differences in medical specialist utilization among older people in need of long-term care - Results from German health claims data},
journal={International Journal for Equity in Health},
year={2020},
