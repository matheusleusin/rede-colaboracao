@CONFERENCE{Zweigel20193022,
author={Zweigel, R. and Gehrt, J.J. and Liu, S. and Roy, S. and Buskens, C. and Kurowski, M. and Jeinsch, T. and Schubert, A. and Gluch, M. and Simanski, O. and Pairet-Garcia, E. and Siemer, F. and Abel, D.},
title={Optimal maneuvering and control of cooperative vehicles as case study for maritime applications within harbors},
journal={2019 18th European Control Conference, ECC 2019},
year={2019},
pages={3022-3027},
doi={10.23919/ECC.2019.8796071},
