@CONFERENCE{Binkele2019,
author={Binkele, T. and Hilbig, D. and Essameldin, M. and Henning, T. and Fleischmann, F. and Lang, W.},
title={Precise measurement of known and unknown freeform surfaces using Experimental Ray Tracing},
