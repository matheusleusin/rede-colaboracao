@CONFERENCE{deOliveira2013,
author={de Oliveira, R.F. and Büskens, C.},
title={Emergency flight replanning for minimum loss of life risk using a decoupled trajectory optimization approach},
journal={2013 Aviation Technology, Integration, and Operations Conference},
year={2013},
note={cited By 1; Conference of 2013 Aviation Technology, Integration, and Operations Conference ; Conference Date: 12 August 2013 Through 14 August 2013;  Conference Code:99255},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84883714620&partnerID=40&md5=2ca9169530fed359589a03853407ee11},
affiliation={EADS Innovation Works, Electronics and Systems Integration, 81663 München, Deutschland, Germany; Zentrum für Technomathematik, Universität Bremen, MZH 2070, Bibliothekstrasse 1, 28359 Bremen, Germany},
abstract={In a response to an emergency situation during flight, the initial focus of the pilots is to stabilize and regain control of the aircraft. Then, their attention is switched to find the shortest and safest way to land the aircraft, with assistance from the flight management system, flight manuals and air traffic controllers, whenever possible. The decision on where to land can be a difficult one, as it might be affected by the new maneuver envelope after the emergency situation is identified, the available fuel and new fuel consumption rate, the distance to the available landing fields, the length of their runways (and thus the maximum allowed landing weight for a non-crashing landing), as well as terrain avoidance and minimizing the risk to the population and properties on the ground, among other factors. In this work, we present an extension to an in-development on-board trajectory optimization framework that is able to propose optimal emergency trajectories, offering the pilot a set of possibilities to land the aircraft in different airports, ranked by risk. As a demonstration of its capabilities, landing trajectories for emergency scenarios minimizing flight risk, ground population and weather hazard risks are obtained for a hypothetical flight emergency situation.},
