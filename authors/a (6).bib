@CONFERENCE{Wernsing2015328,
author={Wernsing, H. and Büskens, C.},
title={Parameter identification for finite element based models in dry machining applications},
journal={Procedia CIRP},
year={2015},
volume={31},
pages={328-333},
doi={10.1016/j.procir.2015.03.037},
note={cited By 6; Conference of 15th CIRP Conference on Modelling of Machining Operations, CMMO 2015 ; Conference Date: 11 June 2015 Through 12 June 2015;  Conference Code:112692},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84939172338&doi=10.1016%2fj.procir.2015.03.037&partnerID=40&md5=3409ef80fcb59298a74d3c8826f3c384},
affiliation={Optimization and Optimal Control, Center for Industrial Mathematics (ZeTeM), MZH, Bibliothekstr. 1, Bremen, 28359, Germany},
abstract={Dry machining is a challenging topic in industrial manufacturing: The absence of coolants results in ecological and economical benefits, but also in a significant increase of the occurring thermal stress. This leads to geometrical deviations of the machined workpiece impairing the functional performance of the final part. The increased thermal stress is especially important in high-precision manufacturing and requires careful process planning. To determine process parameters in order to minimize geometrical errors, optimal control is an appropriate tool. A precise partial differential equation model representing the material behavior is required. The partial differential equation contains unknown physical quantities, such as heat fluxes, which cannot be measured directly. One common method to derive these quantities is solving an inverse problem by parameter identification, i.e. determining the model parameters, such that the model matches the measurements best. We will present a simultaneous analysis and design approach which states a numerical finite element discretization of the model as constraints of the resulting nonlinear optimization problem (NLP). The proposed method takes advantage of both, direct consideration of state constraints, like temperature boundaries, and lower computational costs compared to the nested analysis and design approach. The additional implementation costs can be significantly reduced by using an external finite element library for assembling the optimization constraints and its derivatives. This approach for determining physical parameters of milling experiments is user-independent and works fully automated. It is the first step for an optimal control of dry machining processes. We will give a description of this method as well as numerical identification results of real heat flux densities by the sparse NLP solver WORHP. © 2015 The Authors. Published by Elsevier B.V.},
author_keywords={Finite element method (FEM);  Identification;  Machining;  Modeling;  Optimization},
keywords={Cost benefit analysis;  Differential equations;  Heat flux;  Identification (control systems);  Inverse problems;  Machining;  Machining centers;  Manufacture;  Models;  Natural language processing systems;  Nonlinear programming;  Numerical methods;  Optimization;  Parameter estimation;  Partial differential equations;  Process control;  Thermal stress, Finite-element discretization;  Functional performance;  Geometrical deviations;  High-precision manufacturing;  Industrial manufacturing;  Non-linear optimization problems;  Partial differential equation models;  Simultaneous analysis and designs, Finite element method},
