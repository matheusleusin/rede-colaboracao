@ARTICLE{WohweSambo20205298,
author={Wohwe Sambo, D. and Forster, A. and Yenke, B.O. and Sarr, I. and Gueye, B. and Dayang, P.},
title={Wireless Underground Sensor Networks Path Loss Model for Precision Agriculture (WUSN-PLM)},
journal={IEEE Sensors Journal},
year={2020},
volume={20},
number={10},
pages={5298-5313},
doi={10.1109/JSEN.2020.2968351},
art_number={8964471},
note={cited By 3},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85083845708&doi=10.1109%2fJSEN.2020.2968351&partnerID=40&md5=7115194ebcb90a73eaaaf2f955341499},
affiliation={Faculty of Science, University of Ngaoundéré, Ngaoundéré, 454, Cameroon; ComNets, University of Bremen, Bremen, 28334, Germany; LASE Laboratory, University of Ngaoundéré, Ngaoundéré, 454, Cameroon; Faculty of Sciences and Techniques, University Cheikh Anta Diop, Dakar, 5005, Senegal},
abstract={Despite a large number of applications in the field of health care, military, ecology or agriculture, the Wireless Underground Sensor Network (WUSN) faces the problem of wireless Underground Communication (WUC) which largely attenuate the signal on the ground. For the case of precision agriculture, the motes are buried and they have to check the good growth of plants by verifying data like the water content. However, due to soil composition, the wave signal is attenuated as it travels across the ground. Thus, before a real deployment of WUSN, the prediction of the path loss due to signal attenuation underground is an important asset for the good network functioning. In this paper, we proposed a WUSN path loss for precision agriculture called WUSN-PLM. To achieve it, the proposed model is based on an accurate prediction of the Complex Dielectric Constant (CDC). WUSN-PLM allows evaluating the path loss according to the different types of communication (Underground-to-Underground, Underground to Aboveground and Aboveground to Underground). On each communication type, WUSN-PLM takes into account reflective and refractive wave attenuation according to the sensor node burial depth. To evaluate WUSN-PLM, intensive measurements on real sensor nodes with two different pairs of transceivers have been conducted on the botanic garden of the University Cheikh Anta Diop in Senegal. The results show that the proposed model outperforms the existing path loss models in different communication types. The results show that our proposed approach can be used on real cheap sensor with 87.13% precision and 85% balanced accuracy. © 2001-2012 IEEE.},
author_keywords={complex dielectric constant;  path loss;  precision agriculture;  Wireless underground sensor network},
