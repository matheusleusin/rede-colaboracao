@CONFERENCE{Breiter200491,
author={Breiter, A.},
title={Requirements development in loosely coupled systems: Building a knowledge management system with schools},
journal={Proceedings of the Hawaii International Conference on System Sciences},
year={2004},
volume={37},
pages={91-100},
doi={10.1109/hicss.2004.1265052},
art_number={CLATL10},
note={cited By 2; Conference of Proceedings of the Hawaii International Conference on System Sciences ; Conference Date: 5 January 2004 Through 8 January 2004;  Conference Code:62887},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-12344257767&doi=10.1109%2fhicss.2004.1265052&partnerID=40&md5=65826086276615c7dac7bd64ab9cbe60},
affiliation={Inst. for Info. Management Bremen, Germany},
abstract={The diffusion of computers, local area networks and Internet connections in schools and the increase of instructional software and online resources is a challenge for the school system. Appropriate tools to store, access, distribute and to use those resources in schools are still in their infancy. In order to understand why schools as learning institutions are struggling with knowledge exchange, existing models for knowledge management will be described and adopted to the school context. Based on this framework, the specific functional requirements for of key stake-holders in schools are derived. In order to integrate the requirements into system development for a computer-based knowledge management system, a participatory design process was initiated with three schools. The empirical findings during the process of requirement analysis are documented, focusing on key organizational aspects in schools. Based on empirical evidence, it is suggested that participatory design can be used not only as a method for user-oriented system development but also as a catalyst for reflecting the new role of technology for teaching and learning within an organizational change process.},
keywords={Computer supported cooperative work;  Information management;  Information retrieval;  Information technology;  Internet;  Learning systems;  Local area networks;  Mathematical models;  Online systems;  School buildings;  Students;  Teaching, Digital media;  Knowledge management;  Mobile devices;  User-oriented systems, Knowledge based systems},
