year={2000},
volume={48},
number={6},
pages={273-280},
doi={10.1524/auto.2000.48.6.273},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85041061088&doi=10.1524%2fauto.2000.48.6.273&partnerID=40&md5=f56bf65a1b1ee8eada62adc9d267a283},
