@CONFERENCE{Bäuml2011513,
author={Bäuml, B. and Birbach, O. and Wimböck, T. and Frese, U. and Dietrich, A. and Hirzinger, G.},
title={Catching flying balls with a mobile humanoid: System overview and design considerations},
