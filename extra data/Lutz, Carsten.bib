@CONFERENCE{Basulto2011191,
author={Basulto, V.G. and Jung, J.C. and Lutz, C. and Schroder, L.},
title={The complexity of probabilistic εL},
journal={CEUR Workshop Proceedings},
year={2011},
volume={745},
pages={191-201},
note={cited By 0; Conference of 24th International Workshop on Description Logics, DL 2011 ; Conference Date: 13 July 2011 Through 16 July 2011;  Conference Code:95134},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84872964333&partnerID=40&md5=f0c6fe96ba6183d9413e0cff64f10ced},
affiliation={University Bremen, Germany; DFKI Bremen, Germany},
abstract={We analyze the complexity of subsumption in probabilistic variants of the description logic EL. In the case where probabilities apply only to concepts, we map out the borderline between tractability and EXPTIME-completeness. One outcome is that any probability value except zero and one leads to intractability in the presence of general TBoxes, while this is not the case for classical TBoxes. In the case where probabilities can also be applied to roles, we show PSPACEcompleteness. This result is (positively) surprising as the best previously known upper bound was 2-EXPTIME and there were reasons to believe in completeness for this class.},
keywords={Description logic;  Probabilistic variant;  Upper Bound, Formal languages;  Probability, Data description},
references={Baader, F., Terminological cycles in a description logic with existential restrictions (2003) Proc. of the 18th Int. Joint Conf. on Artif. Intell. (IJCAI03, pp. 325-330. , Morgan Kaufmann; Baader, F., Brandt, S., Lutz, C., Pushing the EL envelope (2005) Proc. of the 19th Int. Joint Conf. on Artif. Intell. (IJCAI05, pp. 364-369. , Morgan Kaufmann; Baader, F., Calvanese, D., McGuinness, D.L., Nardi, D., Patel-Schneider, P.F., (2003) The Description Logic Handbook. Cambridge, , University Press; Bacchus, F., (1990) Representing and Reasoning with Probabilistic Knowledge, , MIT Press Cambridge, MA; Da Costa, P.C.G., Laskey, K.B., PR-OWL: A framework for probabilistic ontologies (2006) Formal Ontology in Information Systems (FOIS06). Frontiers in Artif. Intell. and Applic, 150, pp. 237-249. , IOS Press; Gutierrez-Basulto, V., Jung, J.C., Lutz, C., Schröder, L., A closer look at the probabilistic description logic Prob-EL (2011) Proc. of the 25th AAAI Conf. on Artif. Intell. (AAAI11 AAAI Press; Halpern, J.Y., (2003) Reasoning about Uncertainty, , MIT Press; Halpern, J.Y., Rabin, M.O., A logic to reason about likelihood (1987) Artif. Intell, 32, pp. 379-405; Herzig, A., Modal probability, belief, and actions (2003) Fund. Inf, 57, pp. 323-344; Jaeger, M., Probabilistic reasoning in terminological logics (1994) Proc. of the 4th Int. Conf. on Princ. of Knowledge Repres. and Reas, pp. 305-316. , (KR94. Morgan Kaufmann; Kazakov, Y., Consequence-driven reasoning for Horn SHIQ ontologies (2009) Proc. of the 21st Int. Joint Conf. on Artif. Intell. (IJCAI09, pp. 2040-2045; Lukasiewicz, T., Tractable probabilistic description logic programs Proc. of the 1st Int. Conf. in Scalable Uncertainty Management (SUM07, 4772, pp. 143-156. , LNCS. Springer Verlag; Lukasiewicz, T., Expressive probabilistic description logics (2008) Artif. Intell, 172, pp. 852-883; Lukasiewicz, T., Straccia, U., Managing uncertainty and vagueness in description logics for the semantic web (2008) J. W