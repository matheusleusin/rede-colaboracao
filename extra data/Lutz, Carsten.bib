doi={10.3233/978-1-58603-891-5-25},
note={cited By 18; Conference of 18th European Conference on Artificial Intelligence, ECAI 2008 ; Conference Date: 21 July 2008 Through 25 July 2008;  Conference Code:139245},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85051930104&doi=10.3233%2f978-1-58603-891-5-25&partnerID=40&md5=898f1be7fba0646c03c9ba3244c556f5},
