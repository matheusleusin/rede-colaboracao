@ARTICLE{Botoeva201727,
author={Botoeva, E. and Konev, B. and Lutz, C. and Ryzhikov, V. and Wolter, F. and Zakharyaschev, M.},
title={Inseparability and conservative extensions of description logic ontologies: A survey},
