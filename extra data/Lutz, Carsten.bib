@CONFERENCE{Glimm2007399,
author={Glimm, B. and Horrocks, I. and Lutz, C. and Sattler, U.},
title={Conjunctive query answering for the description logic SHIQ},
journal={IJCAI International Joint Conference on Artificial Intelligence},
year={2007},
pages={399-404},
note={cited By 74; Conference of 20th International Joint Conference on Artificial Intelligence, IJCAI 2007 ; Conference Date: 6 January 2007 Through 12 January 2007;  Conference Code:97873},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84880883540&partnerID=40&md5=022c7fbfb75a6789817251a56b3da552},
affiliation={School of Computer Science, University of Manchester, United Kingdom; Institute for Theoretical Computer Science, TU Dresden, Germany},
abstract={Conjunctive queries play an important role as an expressive query language for Description Logics (DLs). Although modern DLs usually provide for transitive roles, it was an open problem whether conjunctive query answering over DL knowledge bases is decidable if transitive roles are admitted in the query. In this paper, we consider conjunctive queries over knowledge bases formulated in the popular DL SHIQ and allow transitive roles in both the query and the knowledge base. We show that query answering is decidable and establish the following complexity bounds: regarding combined complexity, we devise a deterministic algorithm for query answering that needs time single exponential in the size of the KB and double exponential in the size of the query. Regarding data complexity, we prove co-NP-completeness.},
keywords={Combined complexity;  Complexity bounds;  Conjunctive queries;  Description logic;  Deterministic algorithms;  Double exponential;  Knowledge basis;  Query answering, Algorithms;  Artificial intelligence;  Data description;  Formal languages;  Knowledge based systems, Query languages},
