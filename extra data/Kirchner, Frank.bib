@CONFERENCE{DeGea20081365,
author={De Gea, J. and Kirchner, F.},
title={Contact impedance adaptation via environment identification},
journal={IEEE International Symposium on Industrial Electronics},
year={2008},
pages={1365-1370},
doi={10.1109/ISIE.2008.4677175},
art_number={4677175},
note={cited By 2; Conference of 2008 IEEE International Symposium on Industrial Electronics, ISIE 2008 ; Conference Date: 30 June 2008 Through 2 July 2008;  Conference Code:74842},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-57849162866&doi=10.1109%2fISIE.2008.4677175&partnerID=40&md5=5e52763c694452ffe473f4edab7136c9},
affiliation={Robotics Group, University of Bremen, Germany; DFKI, German Research Center for Artificial Intelligence, Robotics Lab., Bremen, Germany},
abstract={In this paper we present the results of an approach for identifying the environment using Bayesian inference methods. Using this information, the contact properties between a robotic manipulator and a particular scenario are regulated by means of an impedance controller that adapts to the identified environment. Off-line, the robot records sensory data from a set of possible environments and computes their likelihood functions to be used in a Bayesian estimation model. Online, the robot contacts an environment, computes the posterior probabilities using Bayes' rules, and determines the environment with highest confidence. This information modifies the behaviour of an impedance controller that regulates the robot-environment contact interaction. Simulation and experimental results with an industrial robotic manipulator (Mitsubishi PA-10) are shown that depict the performance of the presented approach. © 2008 IEEE.},
keywords={Bayesian networks;  Flexible manipulators;  Industrial electronics;  Inference engines;  Manipulators;  Parameter estimation;  Robotics, Bayes' rules;  Bayesian estimation models;  Bayesian inferences;  Contact impedances;  Contact interactions;  Contact properties;  Environment identifications;  Impedance controllers;  Likelihood functions;  Mitsubishi;  Posterior probabilities;  Robotic manipulators;  Sensory datums, Robots},
references={Wolpert, D.M., Ghahramani, Z., Computational principles of movement neuroscience (2000) Nature Neuroscience Supplement, 3. , November; Krding, K.P., Ku, S., Wolpert, D.M., Bayesian Integration in Force Estimation (2004) J. Neurophysiology, 92, p. 31613165; Osu, R., Kamimura, N., Iwasaki, H., Nakano, E., Harris, C.M., Wada, Y., Kawato, M., Optimal impedance control for task achievement in the presence of signal-dependent noise (2004) journal of Neurophysiology, 92, p. 11991215; Petrovskaya, A.; Khatib, O.; Thrun, S.; Ng, A. Y. Bayesian estimation for autonomous object manipulation based on tactile sensors Proceedings IEEE International Conference on Robotics and Automation (ICRA 2006), pp. 707-714, 2006; P. Slaets, J. Rutgeerts, K. Gadeyne, T. Lefebvre, H. Bruyninckx, and J. De Schutter. Construction of a Geometric 3-D Model from Sensor Measurements Collected during Compliant Motion. In Proceedings of the Internationa] Symposium on Experimental Robotics, 2004; Hogan, N., Control of contact in robots and biological systems (1992) IEEE Engineering in Medicine and Biology Magazine, 11 (4), pp. 81-82; Franklin, D.W., Osu, R., Burdet, E., Kawato, M., Milner, T.E., Adaptation to stable and unstable environments achieved by combined impedance control and inverse dynamics model (2003) Journal of Neurophysiology, 90, pp. 3270-3282; Jung, S., Hsia, T.C., Neural network impedance force control of robot manipulator, in (1998) IEEE Transactions on Industrial Electronics, 45 (3), pp. 451-461; Seraji, H., Colbaugh, R., Adaptive force-based impedance control (1993) Proceedings of the IEEE International Conference on Intelligent Robots and Systems, 3, pp. 1537-1544; Lu, W.-S., Meng, Q.-H., Impedance control with adaptation for robotic manipulations, in (1991) IEEE Transactions on Robotics and Automation, 7 (3), pp. 408-415; T. E. Milner, D. W. Franklin. Impedance control and internal model use during the initial stage of adaptation to novel dynamics in humans. The Journal