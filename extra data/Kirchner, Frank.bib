@CONFERENCE{Petereit2019249,
author={Petereit, J. and Beyerer, J. and Asfour, T. and Gentes, S. and Hein, B. and Hanebeck, U.D. and Kirchner, F. and Dillmann, R. and Gotting, H.H. and Weiser, M. and Gustmann, M. and Egloffstein, T.},
