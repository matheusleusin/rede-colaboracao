@CONFERENCE{Teuber2019,
author={Teuber, J. and Weller, R. and Buinhas, L. and Kühn, D. and Dittmann, P. and Srinivas, A. and Kirchner, F. and Förstner, R. and Funke, O. and Zachmann, G.},
title={VAMEX-VTB a modular virtual tesbed for multimodal autonomous planetary missions},
