@ARTICLE{Kassahun2007205,
author={Kassahun, Y. and Metzen, J.H. and De Gea, J. and Edgington, M. and Kirchner, F.},
title={A general framework for encoding and evolving neural networks},
journal={Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
year={2007},
volume={4667 LNAI},
pages={205-219},
doi={10.1007/978-3-540-74565-5_17},
note={cited By 4; Conference of 30th Annual German Conference on Artificial Intelligence, KI 2007 ; Conference Date: 10 September 2007 Through 13 September 2007;  Conference Code:70942},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-38149105156&doi=10.1007%2f978-3-540-74565-5_17&partnerID=40&md5=5051a0eedc4cb6afeaad81818014bcbd},
affiliation={Robotics Group, University of Bremen, Robert-Hooke-Str. 5, D-28359, Bremen, Germany; German Research Center for Artificial Intelligence (DFKI), Robert-Hooke-Str. 5, D-28359, Bremen, Germany},
abstract={In this paper we present a novel general framework for encoding and evolving networks called Common Genetic Encoding (CGE) that can be applied to both direct and indirect encoding methods. The encoding has important properties that makes it suitable for evolving neural networks: (1) It is complete in that it is able to represent all types of valid phenotype networks. (2) It is closed, i. e. every valid genotype represents a valid phenotype. Similarly, the encoding is closed under genetic operators such as structural mutation and crossover that act upon the genotype. Moreover, the encoding's genotype can be seen as a composition of several subgenomes, which makes it to inherently support the evolution of modular networks in both direct and indirect encoding cases. To demonstrate our encoding, we present an experiment where direct encoding is used to learn the dynamic model of a two-link arm robot. We also provide an illustration of how the indirect-encoding features of CGE can be used in the area of artificial embryogeny. © Springer-Verlag Berlin Heidelberg 2007.},
keywords={Encoding (symbols);  Feature extraction;  Knowledge representation;  Network architecture, Common Genetic Encoding (CGE);  Genetic operators;  Indirect encoding methods;  Modular networks;  Phenotype networks, Neural networks},
