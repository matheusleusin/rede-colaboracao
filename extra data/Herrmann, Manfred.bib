@ARTICLE{Miedl2010165,
author={Miedl, S.F. and Fehr, T. and Meyer, G. and Herrmann, M.},
title={Neurobiological correlates of problem gambling in a quasi-realistic blackjack scenario as revealed by fMRI},
journal={Psychiatry Research - Neuroimaging},
year={2010},
volume={181},
number={3},
