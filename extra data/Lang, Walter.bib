@CONFERENCE{Hübner2020,
author={Hübner, M. and Koerdt, M. and Hardi, E. and Herrmann, A.S. and Lang, W.},
title={Embedding miniaturized flexible sensors for online monitoring of fibre composite production},
journal={ECCM 2018 - 18th European Conference on Composite Materials},
year={2020},
note={cited By 0; Conference of 18th European Conference on Composite Materials, ECCM 2018 ; Conference Date: 24 June 2018 Through 28 June 2018;  Conference Code:155810},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85084160105&partnerID=40&md5=d47daa1e3eae31753607bb0b36e00d83},
affiliation={Institute for Microsensors,-actuators and-systems (IMSAS), University Bremen, Otto-Hahn-Allee 1, Bremen, 28359, Germany; Faserinstitut Bremen e.V., Am Biologischen Garten 2, Bremen, 28359, Germany; Microsystems Center Bremen, University Bremen, Otto-Hahn-Allee 1, Bremen, 28359, Germany},
abstract={Online monitoring of the production of carbon fibre reinforced polymers (CFRPs) is an enabling technology for fast and reliable manufacturing of CFRP parts. Especially for complex shaped parts, sensors embedded inside the CFRP part, not only in the mould, are necessary for successful monitoring. As the embedded sensors remain inside the part also during the whole lifetime, it is necessary that they do not influence the structural integrity of the CFRP part. Therefore, we embedded different types of inlays representing sensors to evaluate the influence of the inlay on the mechanical properties of the CFRP part. With the fabricated specimens three-point bending tests and interlaminar shear tests were performed. A polyimide foil with holes showed the minimum influence while a silicon inlay showed a significant reduction of the mechanical stability. A miniaturized flexible dielectric sensor, which has a polyimide substrate with holes, was fabricated. With this sensor a successful cure monitoring inside the CFRP is possible. © CCM 2020 - 18th European Conference on Composite Materials. All rights reserved.},
author_keywords={Material integrated sensor;  Mechanical evaluation;  Miniaturized dielectric sensor;  Online monitoring},
