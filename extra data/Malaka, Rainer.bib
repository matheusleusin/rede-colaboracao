@ARTICLE{Smeddinck2017218,
author={Smeddinck, J. and Alexandrovsky, D. and Wenig, D. and Zimmer, M. and Wegele, W. and Juergens, S. and Malaka, R.},
title={Hoverboard: A leap to the future of locomotion in VR!?},
