@CONFERENCE{Boldt2018143,
author={Boldt, M. and Bonfert, M. and Lehne, I. and Cahnbley, M. and Korschinq, K. and Bikas, L. and Finke, S. and Hanci, M. and Kraft, V. and Liu, B. and Nguyen, T. and Panova, A. and Singh, R. and Steenbergen, A. and Malaka, R. and Jan Smeddinck, J.},
title={You Shall Not Pass: Non-Intrusive Feedback for Virtual Walls in VR Environments with Room-Scale Mapping},
journal={25th IEEE Conference on Virtual Reality and 3D User Interfaces, VR 2018 - Proceedings},
year={2018},
pages={143-150},
doi={10.1109/VR.2018.8446177},
art_number={8446177},
note={cited By 2; Conference of 25th IEEE Conference on Virtual Reality and 3D User Interfaces, VR 2018 ; Conference Date: 18 March 2018 Through 22 March 2018;  Conference Code:139093},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85053848102&doi=10.1109%2fVR.2018.8446177&partnerID=40&md5=94b130ee5d3b01131cc66c837dfd451b},
affiliation={University of the Arts Bremen, Germany; ICSI, UC Berkeley, United States},
abstract={Room-scale mapping facilitates natural locomotion in virtual reality (VR), but it creates a problem when encountering virtual walls. In traditional video games, player avatars can simply be prevented from moving through walls. This is not possible in VR with room-scale mapping due to the lack of physical boundaries. Game design is either limited by avoiding walls, or the players might ignore them, which endangers the immersion and the overall game experience. To prevent players from walking through walls, we propose a combination of auditory, visual, and vibrotactile feedback for wall collisions. This solution can be implemented with standard game engine features, does not require any additional hardware or sensors, and is independent of game concept and narrative. A between-group study with 46 participants showed that a large majority of players without the feedback did pass through virtual walls, while 87% of the participants with the feedback refrained from walking through walls. The study found no notable differences in game experience. © 2018 IEEE.},
author_keywords={auditory feedback;  game design;  H.5.2 [Information interfaces and presentation]: User Interfaces.-Interaction styles;  haptic feedback;  K.8.0 [Personal Computing]: General-games;  locomotion;  tactile feedback;  Virtual reality;  virtual walls;  visual feedback},
