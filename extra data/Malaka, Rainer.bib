@CONFERENCE{Muender2019,
author={Muender, T. and Reinschluessel, A.V. and Drewes, S. and Wenig, D. and Döring, T. and Malaka, R.},
title={Does it feel real? Using tangibles with different fidelities to build and explore scenes in virtual reality},
journal={Conference on Human Factors in Computing Systems - Proceedings},
year={2019},
doi={10.1145/3290605.3300903},
note={cited By 2; Conference of 2019 CHI Conference on Human Factors in Computing Systems, CHI 2019 ; Conference Date: 4 May 2019 Through 9 May 2019;  Conference Code:147770},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85067610151&doi=10.1145%2f3290605.3300903&partnerID=40&md5=626f8ed17f4d1ba113b54cadd601aeae},
affiliation={University of Bremen, Digital Media Lab., Bremen, Germany},
abstract={Professionals in domains like film, theater, or architecture often rely on physical models to visualize spaces. With virtual reality (VR) new tools are available providing immersive experiences with correct perceptions of depth and scale. However, these lack the tangibility of physical models. Using tangible objects in VR can close this gap but creates the challenges of producing suitable objects and interacting with them with only the virtual objects visible. This work addresses these challenges by evaluating tangibles with three haptic fidelities: equal disc-shaped tangibles for all virtual objects, Lego-built tangibles, and 3D-printed tangibles resembling the virtual shapes. We present results from a comparative study on immersion, performance, and intuitive interaction and interviews with domain experts. The results show that 3D-printed objects perform best, but Lego offers a good trade-off between fast creation of tangibles and sufficient fidelity. The experts rate our approach as useful and would use all three versions. © 2019 Copyright held by the owner/author(s).},
