@CONFERENCE{Vehmeyer2015276,
author={Vehmeyer, J. and Piotrowska-Kurczewski, I. and Böhmermann, F. and Riemer, O. and Maaß, P.},
title={Least-squares based parameter identification for a function-related surface optimisation in micro ball-end milling.},
