@ARTICLE{Dahlke2008157,
author={Dahlke, S. and Kutyniok, G. and Maass, P. and Sagiv, C. and Stark, H.-G. and Teschke, G.},
title={The uncertainty principle associated with the continuous shearlet transform},
journal={International Journal of Wavelets, Multiresolution and Information Processing},
year={2008},
