@ARTICLE{Trede20126079,
author={Trede, D. and Schiffler, S. and Becker, M. and Wirtz, S. and Steinhorst, K. and Strehlow, J. and Aichler, M. and Kobarg, J.H. and Oetjen, J. and Dyatlov, A. and Heldmann, S. and Walch, A. and Thiele, H. and Maass, P. and Alexandrov, T.},
title={Exploring three-dimensional matrix-assisted laser desorption/ionization imaging mass spectrometry data: Three-dimensional spatial segmentation of mouse kidney},
